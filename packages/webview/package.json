{
  "name": "@rcompat/webview",
  "version": "0.11.3",
  "description": "Standard library webview",
  "bugs": "https://github.com/rcompat/rcompat/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/webview.bin",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rcompat/rcompat",
    "directory": "packages/webview"
  },
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-platforms",
    "test": "npm run build && npx proby",
    "clean": "rm -rf ./lib",
    "copy-platforms": "scripts/copy-platforms.sh",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@rcompat/record": "workspace:^"
  },
  "devDependencies": {
    "esbuild": "^0.25.8"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "apekit": "./src/public/index.ts",
      "default": "./lib/public/index.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
